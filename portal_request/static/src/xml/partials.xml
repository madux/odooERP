<?xml version="1.0" encoding="UTF-8"?>
<templates id="template"  name="alert_dialogs" xml:space="preserve">
<t t-name="alert_dialogs" id="alert_dialogs">
    <div role="dialog" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <header class="modal-header">
                    <h4 class="modal-title"><t t-esc="title"></t></h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">&amp;times;</button>
                </header>
                <main class="modal-body">
                    <t t-esc="msg"></t>
                </main>
                <footer class="modal-footer">
                    <a role="button" href="#" class="btn btn-link btn-sm" data-dismiss="modal">Close</a>
                </footer>
            </div>
        </div>
    </div>
</t>
 
</templates>